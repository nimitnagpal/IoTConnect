<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nimitnagpal.iotconnect">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:theme="@style/Theme.Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FacultyMain" />
        <activity android:name=".StudentMain" />
        <activity android:name=".TaMain" />
        <activity android:name=".MsMain" />
        <activity android:name=".StudentUpdate" />
        <activity android:name=".FacultyUpdate" />
        <activity android:name=".MsUpdate" />
        <activity android:name=".TaUpdate" />
        <activity android:name=".DiscoverFaculty" />
        <activity android:name=".DiscoverStudent"></activity>
    </application>

</manifest>